    "### Test `adjoint`"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3750053b",
   "metadata": {},
   "outputs": [],
   "source": [
    "from cirq_qubitization.quantum_graph.composite_bloq import CompositeBloqBuilder, FancyRegisters\n",
    "\n",
    "bb = CompositeBloqBuilder()\n",
    "q0 = bb.add_register('q0', 1)\n",
    "q1 = bb.add_register('q1', 1)\n",
    "qs, trg = bb.add(And(), ctrl=[q0, q1])\n",
    "(qs,) = bb.add(And(adjoint=True), ctrl=qs, target=trg)\n",
    "cbloq = bb.finalize(q0=qs[0], q1=qs[1])\n",
    "\n",
    "show_bloq(cbloq)"
   ]
  },
  {
   "cell_type": "code",
